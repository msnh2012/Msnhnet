    conv:
      batchNorm: 1
      filters: 1024
      kSize: 3
      stride: 1
      padding: 1
      activation: leaky

    conv:
      batchNorm: 0
      filters: 255
      kSize: 1
      stride: 1
      activation: none

# yolov3_1  [33]
