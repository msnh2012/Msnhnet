config:
  batch: 1
  width: 416
  height: 416
  channels: 3

# conv1 [0]
conv:
  batchNorm: 1
  filters: 32
  kSize: 3
  stride: 1
  padding: 1
  activation: leaky

# conv2 [1]
conv:
  batchNorm: 1
  filters: 64
  kSize: 3
  stride: 2
  padding: 1
  activation: leaky


# residual_block1 [2]
resblock:
  size: 1
  conv:
    batchNorm: 1
    filters: 32
    kSize: 1
    stride: 1
    padding: 0
    activation: leaky

  conv:
    batchNorm: 1
    filters: 64
    kSize: 3
    stride: 1
    padding: 1
    activation: leaky

  activation: none

# conv3 [3]
conv:
  batchNorm: 1
  filters: 128
  kSize: 3
  stride: 2
  padding: 1
  activation: leaky


# residual_block2 [4]
resblock:
  size: 2
  conv:
    batchNorm: 1
    filters: 64
    kSize: 1
    stride: 1
    padding: 0
    activation: leaky

  conv:
    batchNorm: 1
    filters: 128
    kSize: 3
    stride: 1
    padding: 1
    activation: leaky
  
  activation: none

# conv4 [6]
conv:
  batchNorm: 1
  filters: 256
  kSize: 3
  stride: 2
  padding: 1
  activation: leaky

# residual_block3 [7]
resblock:
  size: 8
  conv:
    batchNorm: 1
    filters: 128
    kSize: 1
    stride: 1
    padding: 0
    activation: leaky

  conv:
    batchNorm: 1
    filters: 256
    kSize: 3
    stride: 1
    padding: 1
    activation: leaky
  
  activation: none

# conv5 [15]
conv:
  batchNorm: 1
  filters: 512
  kSize: 3
  stride: 2
  padding: 1
  activation: leaky

# residual_block4 [16]
resblock:
  size: 8
  conv:
    batchNorm: 1
    filters: 256
    kSize: 1
    stride: 1
    padding: 0
    activation: leaky

  conv:
    batchNorm: 1
    filters: 512
    kSize: 3
    stride: 1
    padding: 1
    activation: leaky
  
  activation: none

# conv5 [24]
conv:
  batchNorm: 1
  filters: 1024
  kSize: 3
  stride: 2
  padding: 1
  activation: leaky

# residual_block5 [25]
resblock: 
  size: 4
  conv:
    batchNorm: 1
    filters: 512
    kSize: 1
    stride: 1
    padding: 0
    activation: leaky

  conv:
    batchNorm: 1
    filters: 1024
    kSize: 3
    stride: 1
    padding: 1
    activation: leaky
  
  activation: none


#======================
# add_block1 [29]
addblock:
  size: 2
  branch:
    conv:
      batchNorm: 1
      filters: 512
      kSize: 1
      stride: 1
      activation: leaky

    conv:
      batchNorm: 1
      filters: 1024
      kSize: 3
      stride: 1
      padding: 1
      activation: leaky

# conv6 [31] ?
conv:
  batchNorm: 1
  filters: 512
  kSize: 1
  stride: 1
  activation: leaky

